{
  "name": "akuntansi-keuangan-mobile",
  "version": "1.0.0",
  "description": "Aplikasi Akuntansi Keuangan - Mobile Version",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "dev": "npx react-native start --port=8081",
    "start": "npx react-native start --port=8081",
    "start:reset": "npx react-native start --reset-cache --port=8081",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android-debug": "cd android && ./gradlew assembleDebug",
    "clean": "npx react-native clean-project-auto",
    "clean:full": "npx react-native clean-project-auto && npx react-native start --reset-cache --port=8081",
    "android:dev": "npx react-native start --port=8081 & npx react-native run-android",
    "postinstall": "npx react-native-asset",
    "setup:java": "chmod +x scripts/setup-java.sh && ./scripts/setup-java.sh",
    "setup:java:windows": "scripts\\setup-java.bat",
    "check:env": "echo 'Checking environment...' && java -version && echo 'JAVA_HOME:' $JAVA_HOME"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "^3.29.0",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-vector-icons": "^10.0.3",
    "@supabase/supabase-js": "^2.50.3",
    "react-native-url-polyfill": "^2.0.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react-native-elements": "^3.4.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^14.1.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-picker-select": "^9.1.3",
    "react-native-modal": "^13.0.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react-test-renderer": "^18.0.0",
    "@react-native-community/cli": "latest",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}